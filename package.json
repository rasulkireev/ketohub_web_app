    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.2.1",
    "scssfmt": "^1.0.3",
    "ts-node": "~3.3.0",
    "tslint": "~5.8.0",
    "tslint-config-airbnb": "^5.4.2",
    "typescript": "^2.6.2"
