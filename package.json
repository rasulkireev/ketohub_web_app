{
  "name": "ketohub",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --aot",
    "test": "ng test --no-watch --code-coverage",
    "lint": "ng lint && ./node_modules/.bin/scssfmt --recursive 'src/**/**/*.scss' --diff",
    "e2e": "ng e2e",
    "update-deps": "npm update",
